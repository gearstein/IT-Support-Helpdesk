@{
    ViewData["Title"] = "Home Page";
    Layout = "_DashboardLayoutJ";
}


<!--Datatable For Table Master-->
<div class="table-responsive">
    <table class="table table-striped table-hover mt-4" id="masterData">
        <thead>
            <tr>
                <th scope="col">No</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Phone Number</th>
                <th scope="col">Role Name</th>
            </tr>
        </thead>
        <!--Show isi data table link to ajax js-->
        <tbody id="showData">
        </tbody>
    </table>
</div>


@section Scripts{
    <script type="text/javascript" src="~/js/DashboardJunior.js"></script>
}




